@Article{Denef1975,
  author        = {Denef, J.},
  title         = {{H}ilbert's tenth problem for quadratic rings},
  journal       = {Proc. Amer. Math. Soc.},
  year          = {1975},
  volume        = {48},
  number        = {1},
  pages         = {214--220},
  month         = mar,
  issn          = {0002-9939},
  __markedentry = {[timbenediktherbstrith:5]},
  abstract      = {Let Z[$\sqrt{}$D] be a quadratic extension of the ring Z of rational integers. A k-ary relation R(y1,$\cdots$,yk) over Z[$\sqrt{}$D] is said to be Diophantine if there is a polynomial P in Z[y1,$\cdots$,yk,x1,$\cdots$,xn] such that R(y1,$\cdots$,yk) holds if and only if there exist numbers x1,$\cdots$,xn in Z[$\sqrt{}$D] such that P(y1,$\cdots$,yk,x1,$\cdots$,xn)=0. The author proves that a k-ary relation is Diophantine over Z[$\sqrt{}$D] if and only if it is recursively enumerable and invariant under the automorphism $\sqrt{}$D$\rightarrow$$-$$\sqrt{}$D (conjugation). In particular, he proves that the solvability of polynomial equations over Z[$\sqrt{}$D] is effectively undecidable.
   The author considers separately the cases in which D is positive and negative. In each case he gives a simple set of Diophantine equations in the variables (t,u1,$\cdots$,up) all of whose solutions in Z[$\sqrt{}$D] involve rational integral values for t; moreover, the positive values of t that arise include all squares of natural numbers. Since every natural number is the sum of at most four squares of natural numbers, the relation x$\in$N is Diophantine over Z[$\sqrt{}$D]. Consequently, the author's problem is equivalent to Hilbert's tenth problem, since every Diophantine relation over Z[$\sqrt{}$D] is invariant under conjugation.
Reviewed by George S. Sacerdote},
  doi           = {10.2307/2040720},
  file          = {:Denef1975 - Hilbert's tenth problem for quadratic rings.pdf:PDF},
  fjournal      = {Proceedings of the American Mathematical Society},
  mrclass       = {10N05 (02F50 10B99)},
  mrnumber      = {0360513},
  mrreviewer    = {George S. Sacerdote},
}

@Article{Denef1980,
  author        = {Denef, J.},
  title         = {Diophantine sets over algebraic integer rings. {II}},
  journal       = {Trans. Amer. Math. Soc.},
  year          = {1980},
  volume        = {257},
  number        = {1},
  pages         = {227--236},
  issn          = {0002-9947},
  __markedentry = {[timbenediktherbstrith:5]},
  abstract      = {Let K be an algebraic number field of finite degree and OK its ring of integers. The author considers the question whether the set Z of rational integers is Diophantine over OK, i.e., whether there exists a polynomial P(x,y1,$\cdots$,ym) with coefficients in OK having the following property: An element x$\in$OK is contained in Z if and only if the equation P(x,y1,$\cdots$,ym)=0 is solvable with y1,$\cdots$,ym$\in$OK.
   It is conjectured that this is the case for every number field K. In the present paper the author proves this conjecture when K is totally real and when K is a quadratic extension of a totally real field. The proof relies on the detailed study of the solutions of the Pell equation x2$-$dy2=1, where d=a2$-$1, a$\in$OK.},
