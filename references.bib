  mrnumber      = {549163},
  mrreviewer    = {P. Roquette},
}

@Article{Denef1978,
  author        = {Denef, J. and Lipshitz, L.},
  title         = {Diophantine sets over some rings of algebraic integers},
  journal       = {J. London Math. Soc. (2)},
  year          = {1978},
  volume        = {18},
  number        = {3},
  pages         = {385--391},
  issn          = {0024-6107},
  __markedentry = {[timbenediktherbstrith:5]},
  abstract      = {Let L be an algebraic number field and $\mathcal{O}$ its ring of integers. The authors conjecture that Z is Diophantine over $\mathcal{O}$, i.e., that there exists a polynomial P(x,y1,$\cdots$,ym) with coefficients in $\mathcal{O}$ having the following property: An element a$\in$$\mathcal{O}$ is contained in Z if and only if there exist b1,$\cdots$,bm$\in$$\mathcal{O}$ with P(a,b1,$\cdots$,bm)=0. If the conjecture were true, then the analogue of Hilbert's tenth problem for $\mathcal{O}$ would be unsolvable. In an earlier paper the conjecture was proved for [L:Q]=2 [the first author, Proc. Amer. Math. Soc. 48 (1975), 214--220; MR0360513]. In the present paper, the authors extend the method to find some more number fields for which the conjecture is true (obtaining the above result as a special case). The conjecture is true if [L:Q]=4, L is not totally real, and L contains a quadratic subfield; or if L is not totally real, and is of degree 2 over a totally real subfield for which the conjecture is true. The first author promises to prove in a subsequent paper that the conjecture is true for every totally real number field, and hence, by the above, also for any quadratic extension of a totally real number field.},
  doi           = {10.1112/jlms/s2-18.3.385},
  file          = {:Denef1978 - Diophantine sets over some rings of algebraic integers.pdf:PDF},
  fjournal      = {The Journal of the London Mathematical Society. Second Series},
  keywords      = {rank4},
  mrclass       = {12L05 (03D35 10B99)},
  mrnumber      = {518221},
  mrreviewer    = {P. Roquette},
}

