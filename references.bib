@Article{Pheidas1988,
  author        = {Pheidas, Thanases},
  title         = {{H}ilbert's tenth problem for a class of rings of algebraic integers},
  journal       = {Proc. Amer. Math. Soc.},
  year          = {1988},
  volume        = {104},
  number        = {2},
  pages         = {611--620},
  issn          = {0002-9939},
  __markedentry = {[timbenediktherbstrith:5]},
  abstract      = {A set of n-tuples of a ring R is called Diophantine over R if it is of the form {x∈Rn:(∃y∈Rm)p(x,y)=0} for a polynomial p in R[x,y]. J. Denef and L. Lipshitz [J. London Math. Soc. (2) 18 (1978), no. 3, 385–391; MR0518221] conjectured that for every field K that is a finite extension of Q, the set Z is Diophantine over OK, where OK is the ring of integers of the field K. In the above-cited paper as well as in a number of papers of Denef, the validity of this conjecture for some special cases was proved, among them for quadratic extensions of Q [see, e.g., Proc. Amer. Math. Soc. 48 (1975), 214–220; MR0360513].
   In the present paper, using the methods of Denef [Trans. Amer. Math. Soc. 257 (1980), no. 1, 227–236; MR0549163] the author proves this conjecture for finite extensions of Q of degree ≥3 with exactly two nonreal embeddings into C. (An example of such a field is Q(d), where d3 is a rational number without rational cubic roots.)
   The result, together with Matiyasevich's theorem that recursively enumerable subsets of Z are Diophantine, gives the undecidability of Hilbert's tenth problem for rings of integers of such fields.
Reviewed by O. V. Belegradek},
  doi           = {10.2307/2047021},
  file          = {:Pheidas1988 - Hilbert's tenth problem for a class of rings of algebraic integers.pdf:PDF},
